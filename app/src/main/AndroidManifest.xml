<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.testing">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />


    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.mvp.MVPActivity" />
        <activity android:name="com.android.https.HttpsActivity" />
        <activity android:name="com.android.navigator.NavigatorActivity" />
        <activity android:name="com.android.network.MarsActivity" />
        <activity android:name="com.android.login.LoginActivity" />
        <activity android:name="com.android.loader.LoaderActivity" />
        <activity android:name="com.android.calltele.CallTelActivity" />
        <activity android:name="com.android.webview.WebActivity" />
        <activity android:name="com.android.webview.MovieActivity"/>
    </application>

</manifest>